{
    "manifest_version": 3,
    "name": "Holdet.dk Manager Spil - Helper",
    "version": "0.1",
    "description": "Få stats og nyttig information om alle spillere. Uden at skulle søge det frem selv.",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "js": [
                "scripts/contentScript.js"
            ],
            "matches": [
                "https://www.holdet.dk/*"
            ]
        }
    ],
    "host_permissions": [
        "https://www.holdet.dk/*"
      ]
}